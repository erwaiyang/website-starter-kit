{
  "name": "website-starter-kit",
  "version": "0.1.0",
  "description": "Newest and most popular tools!",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --inline --hot --config webpack.config.dev.js",
    "build": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "test": "NODE_PATH=./src mocha ./test/**/*.js --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint src/**/*.js src/**/*.jsx",
    "stylelint": "stylelint src/styles/*.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erwaiyang/website-starter-kit.git"
  },
  "keywords": [
    "website",
    "boilerplate",
    "react",
    "redux",
    "starter-kit"
  ],
  "author": "Jean Yang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erwaiyang/website-starter-kit/issues"
  },
  "homepage": "https://github.com/erwaiyang/website-starter-kit#readme",
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-saga": "^0.11.0"
  },
  "devDependencies": {
    "babel-core": "^6.13.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.9",
    "babel-preset-es2015": "^6.13.1",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-import-resolver-webpack": "^0.4.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "file-loader": "^0.9.0",
    "image-webpack-loader": "^2.0.0",
    "mocha": "^3.0.1",
    "postcss": "^5.1.1",
    "postcss-cssnext": "^2.7.0",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.3.0",
    "react-hot-loader": "^1.3.0",
    "sinon": "^1.17.5",
    "style-loader": "^0.13.1",
    "stylelint": "^7.1.0",
    "stylelint-config-standard": "^12.0.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
