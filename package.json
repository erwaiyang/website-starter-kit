{
  "name": "website-starter-kit",
  "version": "0.1.0",
  "description": "Newest and most popular tools!",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --inline --hot --config webpack.config.dev.js",
    "build": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "test": "NODE_PATH=./src mocha ./test/**/*.js --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint src/**/*.js src/**/*.jsx",
    "stylelint": "stylelint src/styles/*.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erwaiyang/website-starter-kit.git"
  },
  "keywords": [
    "website",
    "boilerplate",
    "react",
    "redux",
    "starter-kit"
  ],
  "author": "Jean Yang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erwaiyang/website-starter-kit/issues"
  },
  "homepage": "https://github.com/erwaiyang/website-starter-kit#readme",
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "babel-core": "^6.13.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-preset-es2015": "^6.13.1",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "chai": "^4.1.2",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "image-webpack-loader": "^4.2.0",
    "mocha": "^5.1.1",
    "postcss": "^6.0.22",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.5",
    "react-addons-test-utils": "16.0.0-alpha.3",
    "react-hot-loader": "^4.1.2",
    "sinon": "^5.0.7",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "webpack": "^4.8.1",
    "webpack-dev-server": "^3.1.4"
  }
}
